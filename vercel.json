{
  "version": 2,
  "builds": [
    { "src": "./index.js", "use": "@vercel/node" },
    { "src": "./api/download.js", "use": "@vercel/node" },
    { "src": "./public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/download", "dest": "./api/download.js" },
    { "src": "/(.*)", "dest": "./index.js" }
  ]
}